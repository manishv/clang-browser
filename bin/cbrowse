export PYTHONPATH=$PYTHONPATH:$CLANG_PYTHON_DIR
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$LLVM_INSTALL_DIR/lib

python $ROOTDIR/src/python/utils/codebrowse.py $*
