#!/bin/bash

ROOTDIR=/data/work/clang-browser
CLANG_PYTHON_DIR=/data/work/LLVM/llvm-src/tools/clang/bindings/python
LLVM_INSTALL_DIR=/data/work/LLVM/llvm-rel

